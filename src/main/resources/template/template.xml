<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.dlts.hrms.mapper.${entityName}Mapper">

  <sql id="Base_Column_List">
    ${Base_Column}
  </sql>
  
  <select id="get" parameterType="java.lang.String" resultType="com.dlts.hrms.entity.${entityName}">
    select <include refid="Base_Column_List" /> from ${tableName} where id = #{id}
  </select>
  
  <select id="select" parameterType="com.dlts.hrms.entity.${entityName}" resultType="com.dlts.hrms.entity.${entityName}">
    select <include refid="Base_Column_List" /> from ${tableName} ${where}
  </select>
  
  <delete id="delete" parameterType="java.lang.String">
    delete from ${tableName} where id = #{id}
  </delete>
  
  <delete id="deleteSelective" parameterType="com.dlts.hrms.entity.${entityName}">
    delete from ${tableName} ${where}
  </delete>
  
  <insert id="insert" parameterType="com.dlts.hrms.entity.${entityName}">
    insert into sys_area (${Base_Column}) values (${insertValues})
  </insert>
  
  <update id="updateSelective" parameterType="com.dlts.hrms.entity.${entityName}">
    update ${tableName} ${updateSelectiveSet} where id = #{id}
  </update>
  
  <update id="update" parameterType="com.dlts.hrms.entity.${entityName}">
    update ${tableName} set ${updateSet} where id = #{id}
  </update>
  
</mapper>